programa
{
	
	funcao inicio()
	{
		real n1, n2, n3, m

		escreva("Diga um número: ")
		leia(n1)
		escreva("Diga mais um número: ")
		leia(n2)
		escreva("Diz mais umzinho: ")
		leia(n3)
		m = (n1+n2+n3)/3
		escreva("A média desses três números é: ", m)
	}
}
